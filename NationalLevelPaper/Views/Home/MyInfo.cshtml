@model IEnumerable<NationalLevelPaper.Models.User>
    @{
        ViewBag.Title = "MyInfo";
        Layout = "~/Views/Shared/Innerpages.cshtml";
    }

    


<div class="banner-area-wrapper">
    <div class="banner-area text-center">
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    <div class="banner-content-wrapper">
                        <div class="banner-content">
                            <h2>MyInfo</h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


    <div class="container">
        <div class="row">
            @foreach (var item in Model)
            {


                <div class="col-md-12">

                    <form action="/" method="post">

                        <label>Username</label>
                        <input type="text" name="username" value="@item.Name" class="form-control" />


                        <label>Age</label>
                        <input type="text" name="age" value="@item.Age" class="form-control" />

                        <label>Qualification</label>
                        <input type="text" name="quali" value="@item.Qualification" class="form-control" />

                        <label>Address</label>
                        <input type="text" name="address" value="@item.Address" class="form-control" />


                        <label>Phone</label>
                        <input type="text" name="phone" value="@item.Phone" class="form-control" />

                        <label>Email</label>
                        <input type="text" name="email" value="@item.Email" class="form-control" />

                        <label>Password</label>
                        <input type="text" name="pass" value="@item.Password" class="form-control" />
                        <br />
                        <p>if u change anything in your info then u chnage in textbox and click on update button</p>
                        <br />
                          <input type="Submit" name="name" value="Update" />
                    </form> 



                </div>
            }

        </div>

    </div>
